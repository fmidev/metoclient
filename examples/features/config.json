{
  "minZoom": 1,
  "maxZoom": 10,
  "center": [
    385956,
    6671437
  ],
  "zoom": 3,
  "container": "map",
  "projection": "EPSG:3067",
  "refreshInterval": "PT15M",
  "timeZone": "Europe/Helsinki",
  "sources": {
    "osm": {
      "type": "OSM"
    },
    "points": {
      "data": "./features.json",
      "type": "geojson"
    }
  },
  "layers": [
    {
      "id": "basic-map",
      "source": "osm",
      "metadata": {
        "type": "base",
        "title": "Basic map"
      }
    },
    {
      "source": "points",
      "type": "symbol",
      "id": "points",
      "metadata": {
        "timeProperty": "time",
        "title": "Points"
      },
      "layout": {
        "text-field": "{n}",
        "text-size": 14,
        "text-font": ["Arial Normal", "sans-serif Normal"]
      }
    }
  ]
}
