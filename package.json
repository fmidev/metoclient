{
  "name": "metoclient",
  "version": "4.34.3",
  "private": false,
  "description": "Map animation library",
  "main": "src/MetOClient.js",
  "directories": {
    "doc": "doc",
    "example": "examples"
  },
  "executables": {
    "nodejs": "node"
  },
  "scripts": {
    "build": "env-cmd ./config/webpack/env.prod webpack --progress --config config/webpack/config.js",
    "build-dev": "env-cmd ./config/webpack/env.dev webpack --progress --config config/webpack/config.js",
    "build-without-moment": "env-cmd ./config/webpack/env.prod-without-moment webpack --progress --config config/webpack/config.js",
    "build-custom": "webpack --progress --config config/webpack/config.js",
    "test": "cross-env PHANTOMJS_EXECUTABLE=node_modules/.bin/phantomjs; $npm_package_executables_nodejs node_modules/casperjs/bin/casperjs.js test test/wms.js",
    "doc": "node_modules/.bin/del-cli ./build/jsdoc; node_modules/.bin/jsdoc -c config/jsdoc/config.json -d ./build/jsdoc",
    "examples": "$npm_package_executables_nodejs ./example-builder/build-examples.js",
    "release": "release-it"
  },
  "author": "Finnish Meteorological Institute",
  "license": "MIT",
  "homepage": "https://github.com/fmidev/metoclient",
  "dependencies": {
    "core-js": "2.6.5",
    "element-resize-detector": "1.1.15",
    "empty-element": "1.0.0",
    "extend": "3.0.2",
    "fast-isnumeric": "1.1.2",
    "good-listener": "1.2.2",
    "localforage": "1.7.3",
    "moment": "2.24.0",
    "moment-timezone": "0.5.13",
    "mouse-wheel": "1.2.0",
    "ol": "4.3.3",
    "proj4": "2.5.0",
    "rename-keys": "2.0.1",
    "shallowequal": "1.1.0",
    "wolfy87-eventemitter": "5.2.6"
  },
  "devDependencies": {
    "async": "2.0.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-array-includes": "2.0.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-webpack-named-dynamic-imports": "1.1.0",
    "babel-preset-env": "1.6.0",
    "casperjs": "1.1.4",
    "copy-webpack-plugin": "4.2.0",
    "cross-env": "5.0.5",
    "del-cli": "1.1.0",
    "env-cmd": "7.0.0",
    "eslint": "4.18.2",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "3.0.0",
    "fs-extra": "0.30.0",
    "handlebars": "4.1.2",
    "jsdoc": "3.6.3",
    "license-webpack-plugin": "1.1.1",
    "marked": "0.7.0",
    "metalsmith": "2.3.0",
    "metalsmith-layouts": "1.6.4",
    "nomnom": "1.8.1",
    "release-it": "12.3.5",
    "slimerjs": "0.10.3",
    "standard": "10.0.3",
    "style-loader": "0.18.2",
    "temp": "0.8.3",
    "walk": "2.3.9",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-conditional-loader": "1.0.7"
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
